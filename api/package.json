{
  "name": "vistoriasync-api",
  "version": "1.0.0",
  "description": "API do VistoriaSync - Sistema de Integração KSI-Flow",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:prod": "npm run build && npm start",
    "worker": "tsx watch src/workers/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.1",
    "dayjs": "^1.11.10",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "pino-pretty": "^13.1.3",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1"
  }
}
